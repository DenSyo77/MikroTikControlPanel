{
  "name": "MikroTik Control Panel",
  "description": "Manage routes on MikroTik",
  "version": "1.1",
  "manifest_version": 3,
    "icons": {
      "16": "icons/logo16.png",
      "32": "icons/logo32.png",
      "48": "icons/logo48.png",
      "128": "icons/logo128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
